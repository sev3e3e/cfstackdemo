{
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": "."
  },
  "include": [
    "frontend",
    "backend/**/*",
    "lightweight-otel-sdk",
    "logger",
    "utility",
    "types"
  ],
  "exclude": []
}
