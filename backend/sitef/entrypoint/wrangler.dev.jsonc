/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cfstackdemo-sitef-entrypoint",
	"main": "src/index.dev.ts",
	"compatibility_flags": ["nodejs_compat"],
	"compatibility_date": "2025-07-11",
	"observability": {
		"enabled": true
	},
	"triggers": {
		"crons": ["0 */2 * * *"]
	},
	"queues": { "producers": [{ "binding": "DemoScrapingQueue", "queue": "cfstackdemo-scraping-queue" }] },
	"services": [{ "binding": "SitefFetcher", "service": "cfstackdemo-sitef-fetcher" }],
	"vars": {
		"ENVIRONMENT": "development"
	},
	"env": {
		"example": {
			"vars": {
				"ENVIRONMENT": "example"
			},
			"queues": { "producers": [{ "binding": "DemoScrapingQueue", "queue": "cfstackdemo-scraping-queue" }] },
			"services": [{ "binding": "SitefFetcher", "service": "cfstackdemo-sitef-fetcher" }]
		},
		"test": {
			"vars": {
				"ENVIRONMENT": "test"
			},
			"queues": { "producers": [{ "binding": "DemoScrapingQueue", "queue": "cfstackdemo-scraping-queue" }] },
			"services": [{ "binding": "SitefFetcher", "service": "cfstackdemo-sitef-fetcher" }]
		}
	}
}
