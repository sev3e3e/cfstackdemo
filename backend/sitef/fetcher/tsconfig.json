{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"jsx": "react",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"types": ["@cloudflare/workers-types/experimental", "@cloudflare/vitest-pool-workers", "./worker-configuration.d.ts"],
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"isolatedModules": false,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noErrorTruncation": true
	}
}
