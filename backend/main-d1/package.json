{
	"name": "@cfstackdemo/main-d1",
	"version": "0.0.0",
	"private": true,
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"deploy": "wrangler deploy --minify -c ./wrangler.jsonc -e production --keep-vars",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "vitest --watch=false",
		"cf-typegen": "wrangler types",
		"db:gen": "drizzle-kit generate",
		"db:mig-local": "wrangler d1 migrations apply cfstackdemo-main-d1 --local",
		"db:mig-prod": "wrangler d1 migrations apply cfstackdemo-main-d1 --remote",
		"db:reset": "node cleandb.mjs",
		"db:seed": "node movefile.mjs",
		"db:dbg": "run-s db:reset db:seed db:gen db:mig-local",
		"analyze": "wrangler deploy --minify -c ./wrangler.jsonc --dry-run --metafile=./meta.json && pnpm esbuild-visualizer --metadata ./meta.json && powershell.exe start stats.html"
	},
	"devDependencies": {
		"@cfstackdemo/types": "workspace:*",
		"@cloudflare/vitest-pool-workers": "catalog:",
		"@cloudflare/workers-types": "catalog:",
		"drizzle-kit": "catalog:",
		"esbuild-visualizer": "catalog:",
		"neverthrow": "catalog:",
		"npm-run-all2": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:",
		"wrangler": "catalog:"
	},
	"dependencies": {
		"@cfstackdemo/lightweight-otel-sdk": "workspace:*",
		"@cfstackdemo/logger": "workspace:*",
		"@cfstackdemo/utility": "workspace:*",
		"drizzle-orm": "catalog:"
	}
}
