{
  "name": "@cfstackdemo/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --mode example",
    "build": "vite build",
    "preview": "pnpm run build && wrangler dev",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "pnpm run build && wrangler deploy -c ./wrangler.jsonc --minify --keep-vars",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
    "start:d1": "wrangler dev -c ../backend/main-d1/wrangler.jsonc  -e example",
    "delay1": "delay 1",
    "delay2": "delay 2",
    "_start:r2": "wrangler dev -c ../backend/main-r2/wrangler.jsonc -e example",
    "start:r2": "run-s delay1 _start:r2",
    "start:frontend": "run-s delay2 dev:frontend",
    "_dev": "concurrently 'pnpm run start:r2' 'pnpm run start:d1' 'pnpm run start:frontend'"
  },
  "devDependencies": {
    "@internationalized/date": "^3.8.2",
    "@lucide/svelte": "0.515.0",
    "@sveltejs/adapter-auto": "6.0.2",
    "@sveltejs/adapter-cloudflare": "7.1.3",
    "@sveltejs/kit": "2.27.3",
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/table-core": "^8.21.3",
    "concurrently": "^9.2.0",
    "delay-cli": "^2.0.0",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "^2.0.1",
    "layerchart": "2.0.0-next.27",
    "mode-watcher": "^1.1.0",
    "npm-run-all2": "catalog:",
    "paneforge": "^1.0.2",
    "svelte": "5.38.0",
    "svelte-check": "4.3.1",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.27.1",
    "tailwind-variants": "1.0.0",
    "tailwindcss": "4.1.11",
    "tw-animate-css": "1.3.6",
    "typescript": "catalog:",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "7.1.1",
    "wait-on": "^8.0.4",
    "wrangler": "catalog:"
  },
  "dependencies": {
    "@axiomhq/js": "1.3.1",
    "@cfstackdemo/main-d1": "workspace:*",
    "@cfstackdemo/main-r2": "workspace:*",
    "@types/d3": "^7.4.3",
    "bits-ui": "2.9.2",
    "clsx": "2.1.1",
    "d3": "^7.9.0",
    "tailwind-merge": "3.3.1"
  }
}
